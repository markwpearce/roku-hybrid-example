<?xml version="1.0" encoding="utf-8" ?>
<component name="MainScene" extends="Scene">
  <script type="text/brightscript" uri="MainScene.brs" />
  <interface>
    <field id="switchCount" type="integer" value="0" onChange="onSwitchCountSet" />
    <field id="switchMode" type="boolean" value="false" />
  </interface>
  <children>
    <Label id="title" translation="[200,100]" text="This is SceneGraph. Press Back to Change Modes" />
    <Label id="switchCountLabel" translation="[200,150]"  />
    <Poster id="rokuLogo" uri="pkg:/images/rokuLogo.jpg" translation="[900,100]" width="200" height="200" />
     <Video id="videoPlayer" translation="[200,200]" width="640"  height="360"/>
  </children>
</component>
